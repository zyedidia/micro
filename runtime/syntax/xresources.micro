syntax "xresources" "X(defaults|resources)$"

color special     "^[[:alnum:]]+\*"
color identifier.var "\*[[:alnum:]]+\:"
color constant.number   "\<[0-9]+\>"
color symbol.operator "[*:=]"
color constant.bool "\<(true|false)\>"
color comment "(^|[[:space:]])#([^{].*)?$"
color indent-char.whitespace "[[:space:]]+$"
color indent-char "	+ +| +	+"
