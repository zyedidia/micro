syntax "inputrc" "inputrc$"

color constant.bool.false   "\<(off|none)\>"
color constant.bool.true "\<on\>"
color preproc   "\<set|\$include\>"
color constant.string ""(\\.|[^"])*"|'(\\.|[^'])*'"
color constant.specialChar     "\\.?"
color comment "(^|[[:space:]])#([^{].*)?$"
color indent-char.whitespace "[[:space:]]+$"
color indent-char "	+ +| +	+"
