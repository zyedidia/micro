syntax "keymap" "\.(k|key)?map$|Xmodmap$"

color statement  "\<(add|clear|compose|keycode|keymaps|keysym|remove|string)\>"
color statement  "\<(control|alt|shift)\>"
color constant.number   "\<[0-9]+\>"
color symbol "="
color constant.string ""(\\.|[^"])*"|'(\\.|[^'])*'"
